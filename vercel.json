{
    "version": 2,
    "builds": [
        {
            "src": "web_bot.py",
            "use": "@vercel/python@3.1.0"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "web_bot.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTHONUTF8": "1"
    }
}
